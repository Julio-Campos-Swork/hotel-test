---
interface Props {
    imageWebp: string;
    imageJPG: string;
    imageName: string;
    clase: string;

}
const {imageWebp, imageJPG, imageName, clase} = Astro.props
---

<picture class={clase}>
    <source srcset={imageWebp} type="image/webp" />
    <img alt={imageName} src={imageJPG}  />
    </picture>